<?xml version='1.0' encoding='ISO-8859-1'?>

<!--
	This file stores the translations
	for all the dynamic labels used by
	the application in all its templates,
	for each language supported
-->

<labels>

	<!-- default labels -->
	<label id="wait">
		<value language="es">
			Por favor espere...
		</value>
		<value language="en">
			Please wait...
		</value>
	</label>
	
	<label id="system_error_title">
		<value language="es">
			Error en la aplicación
		</value>
		<value language="en">
			System Error
		</value>
	</label>

	<label id="system_error_msg">
		<value language="es">
			Lamentamos informarle que ocurrió un error en el sistema.
		</value>
		<value language="en">
			Sorry, an unexpected error has occured in this application.
		</value>
	</label>

	<label id="button_back">
		<value language="es">
		Regresar
		</value>
		<value language="en">
		Continue
		</value>
	</label>

	<label id="invalid_form_title">
		<value language="es">
			El formulario necesita las siguientes correcciones:
		</value>
		<value language="en">
			Form data needs to be fixed:
		</value>
	</label>

	<label id="parameter_required">
		<value language="es">
			Este valor DEBE ser ingresado.
		</value>
		<value language="en">
			This value cannot be empty.
		</value>
	</label>
	
	<label id="invalid_integer">
		<value language="es">
			El dato ingresado no representa un número entero. Tipee un número sin decimales ni letras por favor.
		</value>
		<value language="en">
			Invalid number format. Please type an Integer number.
		</value>
	</label>

	<label id="invalid_double">
		<value language="es">
			El dato ingresado no representa un número. Tipee un número y use el punto (.) como separador de decimales.
		</value>
		<value language="en">
			Invalid number format. Please type a number, may use decimals too.
		</value>
	</label>

	<label id="invalid_date">
		<value language="es">
			El dato ingresado no representa una fecha.
		</value>
		<value language="en">
			Invalid date format.
		</value>
	</label>

	<label id="data_too_long">
		<value language="es">
			Demasiado texto. El dato ingresado excede el tamaño permitido:
		</value>
		<value language="en">
			Text too long, please delete some text. Text size in characters can't be larger than:
		</value>
	</label>

	<label id="min_value_violation">
		<value language="es">
			El valor ingresado está por debajo del mínimo permitido:
		</value>
		<value language="en">
			Value can't be less than:
		</value>
	</label>

	<label id="max_value_violation">
		<value language="es">
			El valor ingresado está por encima del máximo permitido:
		</value>
		<value language="en">
			Value can't be greater than:
		</value>
	</label>


	<!-- security related labels -->

	<label id="s_session_terminated">
		<value language="es">
			Su sesión ha finalizado.
		</value>
		<value language="en">
			Your session has terminated.
		</value>
	</label>

	<label id="s_login_again">
		<value language="es">
			Ingresar nuevamente
		</value>
		<value language="en">
			Login again
		</value>
	</label>

	<label id="s_newpass_alert">
		<value language="es"><![CDATA[Por motivos de seguridad se le solicita que cambie su contraseña en este momento.]]>
		</value>
		<value language="en"><![CDATA[Dear Sir/Madam:<br><br>For <span style="color:red"><b>security reasons</b></span> you will have to change your password. This is a security policy enforced by the application to protect your valuable information resources. If you have questions please consult with the system administrator.<br>]]>
		</value>
	</label>

	<label id="s_form_title">
		<value language="es">
			Cambio de Contraseña
		</value>
		<value language="en">
			Change your Password
		</value>
