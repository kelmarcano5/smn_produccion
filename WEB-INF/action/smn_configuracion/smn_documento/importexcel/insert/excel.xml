<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='smn_tipo_documento_id' type='integer' required='false' maxlen='10'/>
	<parameter id='doc_codigo' type='integer' required='true' maxlen='10'/>
	<parameter id='doc_nombre' type='integer' required='true' maxlen='10'/>
	<parameter id='smn_documento_general_rf' type='integer' required='true' maxlen='10'/>
	<parameter id='smn_modulo_origen' type='integer' required='true' maxlen='10'/>
	<parameter id='doc_tipo_movimiento' type='varchar' required='false' maxlen='2'/>
	<parameter id='doc_tipo_orden' type='varchar' required='false' maxlen='2'/>
	<parameter id='doc_estatus' type='varchar' required='true' maxlen='2'/>


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='doc_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarNombre.sql'
			id='doc_nombre'/>


</validator>
