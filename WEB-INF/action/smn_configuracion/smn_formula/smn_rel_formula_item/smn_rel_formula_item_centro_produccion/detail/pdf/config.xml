<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />

	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:b_relationship_formula_item_left_production}</pdf-title>
	<pdf-logo url='/images/logo-simone-pdf.png' scale='100' />
	<pdf-file-name>${lbl:b_relationship_formula_item_left_production}.pdf</pdf-file-name>

	<pdf-report pageSize="letter" rotate="false" footer="true" header="true">
		<record recordset='query.sql' title='${lbl:b_relationship_formula_item_left_production}' width='60' spacingBefore='40'>
			<col name='smn_rel_formula_item_id_combo' title='${lbl:b_relationship_formula_item}' align='left' format='######' />
			<col name='smn_centro_produccion_id_combo' title='${lbl:b_left_production}' align='left' format='######' />
			<col name='smn_linea_produccion_id_combo' title='${lbl:b_production_line}' align='left' format='######' />
			<col name='rfc_fecha_registro' title='${lbl:b_store_date}' align='left' format='dd-MM-yyyy' />
		</record>


	</pdf-report>
</config>
